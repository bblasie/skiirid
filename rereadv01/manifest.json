{
  "manifest_version": 2,
  "name": "reread web extension proto1",
  "description": "This extension allows for the information you are serfing to get served in an easy and memorable waysss.",
  "version": "2.0",
  "content_scripts": [{
    "matches":[
      "en.wikipedia.org/"
    ],
    "matches":[
      "<all_urls>"
    ],
    "js":["content.js","p5.js","p5.sound.min.js","p5.clickable.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
   "default_icon": "icon2.jpg",
   "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
   "activeTab",
   "storage"
   ]
}
